<%= form_with model: @user, url:user_session_path, class: "form-session", local: true do |f| %>

      <div class="form-header">
        <h1 class="catch-copy">ログイン</h1>
      </div>

      <div class="form-group">
        <div class="form-text-wrap">
          <span class="indispensable">必須</span>
          <label class="form-text">メールアドレス</label>
      </div>
        <%= f.email_field :email, class: "input-default" %>
      </div>

      <div class="form-group">
        <div class="form-text-wrap">
          <span class="indispensable">必須</span>
          <label class="form-text">パスワード</label>
      </div>
        <%= f.password_field :password, class: "input-default" %>
      </div>

      <%= f.submit "ログイン", class: "form-button" %>
       <% end %>